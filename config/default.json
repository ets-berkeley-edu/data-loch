{
  "app": {
    "https": false,
    "port": 1234
  },
  "aws": {
    "credentials": {
      "accessKeyId": "key",
      "secretAccessKey": "secret",
      "region": "aws region"
    }
  },
  "canvasDataApi": {
    "host": "foo.instructure.com",
    "https": true,
    "key": "some key",
    "secret": "some secret"
  },
  "datalake": {
    "s3": {
      "bucket": "bucket name",
      "region": "aws region"
    },
    "canvasData": {
      "directory": {
        "currentTerm": "s3/path/to/current/term/folder",
        "daily": "s3/path/to/daily/folder",
        "historical": "s3/path/to/historical/folder"
      },
      "externalSchema": "external schema name",
      "boacSchema": "boac schema name",
      "iamRole": "iam role",
      "requestsTermRegExp": "regexp for requests file url parsing"
    },
    "suitec": {
      "mixpanel": {
        "key": "mixpanel key",
        "secret": "mixpanel secret",
        "exportParams": {
          "from_date": "start date Eg: 2015-01-10",
          "to_date": "end date"
        }
      },
      "directory": {
        "mixpanelEvents": "path/to/mixpanel/events",
        "suitec": "path/to/suitec/data"
      },
      "externalSchema": "schema_name",
      "analyticsSchema": "analytics_schema_name",
      "stagingSchema": "staging_schema_name",
      "researchGroupRequestingData": "research group name requesting data exteracts"
    },
    "cron": {
        "enabled": false,
        "tasks": {
          "syncDumps" : {
            "interval": "0 0 0/1 * * *",
            "script": "path/to/script",
            "runOnInit": false
          },
          "syncDb" : {
            "interval": "0 0 0/1 * * *",
            "script": "path/to/script",
            "runOnInit": false
          }
        }
    },
    "redshift": {
      "database": "database",
      "host": "redshift cluster",
      "password": "password",
      "port": 1234,
      "user": "username"
    },
    "worker": {
      "beanstalk":{
        "host": "app url",
        "username": "username",
        "password": "password"
      },
      "lambda": {
        "apiVersion": "version",
        "dataSyncService": "sync lambda service name",
        "region": "aws region"
      }
    }
  },
  "logger": {
    "streams": [
      {
        "level": "info",
        "stream": "stdout"
      },
      {
        "level": "debug",
        "path": "logs/app.log"
      }
    ],
    "useColor": false
  }
}
