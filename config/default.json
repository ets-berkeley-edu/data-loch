{
  "aws": {
    "credentials": {
      "accessKeyId": "key",
      "secretAccessKey": "secret",
      "region": "us-east-1"
    },
    "s3": {
      "bucket": "xyz",
      "region": "us-east-1",
      "directory": "xyz/abc"
    }
  },
  "canvas": {
    "apiKey": "apiKey"
  },
  "dataLake": {
    "canvasData": {
      "externalDatabase": "database",
      "s3Location": "s3://path/to/db",
      "iamRole": "iam role"
    },
    "redshiftSpectrum": {
      "user": "username",
      "database": "database",
      "password": "password",
      "port": 5439,
      "host": "redshift cluster"
    }
  },
  "logger": {
    "streams": [
      {
        "level": "info",
        "stream": "stdout"
      },
      {
        "level": "debug",
        "path": "logs/app.log"
      }
    ],
    "useColor": false
  },
  "lrs": {
    "endpoint": "https://localhost/api/statements",
    "username": "username",
    "password": "password"
  },
  "platform": {
    "name": "bCourses",
    "url": "https://bcourses.berkeley.edu"
  },
  "canvasDataApi": {
    "key": "some key",
    "secret": "some secret",
    "host": "foo.instructure.com",
    "https": true
  },
  "statements": {
    "type": "caliper"
  },
  "storage": "/path/to/local/store"
}
