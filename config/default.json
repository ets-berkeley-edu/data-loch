{
  "app": {
    "https": false,
    "port": 2000
  },
  "aws": {
    "credentials": {
      "accessKeyId": "key",
      "secretAccessKey": "secret",
      "region": "us-east-1"
    }
  },
  "canvasDataApi": {
    "host": "foo.instructure.com",
    "https": true,
    "key": "some key",
    "secret": "some secret"
  },
  "datalake": {
    "s3": {
      "bucket": "bucket name",
      "region": "us-east-1"
    },
    "canvasData": {
      "directory": {
        "currentTerm": "s3/path/to/current/term/folder",
        "daily": "s3/path/to/daily/folder",
        "historical": "s3/path/to/historical/folder"
      },
      "externalSchema": "external schema name",
      "iamRole": "iam role"
    },
    "cronJob": {
      "enabled": false,
      "cronTime": "0 * * * *",
      "runOnInit": false
    },
    "redshift": {
      "database": "database",
      "host": "redshift cluster",
      "password": "password",
      "port": 5439,
      "user": "username"
    },
    "worker": {
      "beanstalk":{
        "host": "app url",
        "username": "username",
        "password": "password"
      },
      "lambda": {
        "apiVersion": "2015-03-31",
        "dataSyncService": "sync lambda service name",
        "region": "us-east-1"
      }
    }
  },
  "logger": {
    "streams": [
      {
        "level": "info",
        "stream": "stdout"
      },
      {
        "level": "debug",
        "path": "logs/app.log"
      }
    ],
    "useColor": false
  }
}
